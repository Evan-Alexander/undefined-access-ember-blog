<h1>Welcome to Undefined-Access</h1>
<h3>Where undefined variables are our frienz</h3>
{{new-blog saveBlog2="saveBlog3"}}
<br>
<br>
<ul>
  {{#each model as |blog|}}
    {{blog-tile blog=blog}}
  {{/each}}
</ul>
<hr>
  <h2>Comments</h2>
  <ul>
    {{#each model.comments as |comment|}}
    <div class="row">
      <div class="col-lg-6">
        <li>{{comment.author}}</li>
      </div>
      <div class="col-lg-6">
        {{comment.content}}
      </div>
  </div>
    {{/each}}
  </ul>
